{
  "name": "@mallowigi/eslint-config",
  "version": "1.8.57",
  "author": "Elior Boukhobza <mallowigi@github.com>",
  "license": "MIT",
  "description": "Mallowigi's ESLint config",
  "private": false,
  "homepage": "https://github.com/mallowigi/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mallowigi/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/mallowigi/eslint-config/issues"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "src",
    "!*.test.js"
  ],
  "main": "index.js",
  "exports": "./index.js",
  "keywords": [
    "javascript",
    "typescript",
    "ecmascript",
    "eslint",
    "lint",
    "config",
    "prettier"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "test": "npm run lint",
    "major": "npm version major",
    "minor": "npm version minor",
    "patch": "npm version patch",
    "pub": "npm publish"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "cache": "cache/.npm"
  },
  "badges": {
    "list": [
      "npmversion"
    ]
  },
  "dependencies": {
    "@babel/core": "7.17.2",
    "@babel/eslint-parser": "7.16.5",
    "@babel/preset-react": "7.16.7",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-i18n-json": "3.1.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-lodash": "7.3.0",
    "eslint-plugin-mdx": "1.16.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-react-i18n": "1.0.3",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sonarjs": "0.11.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "eslint-plugin-unicorn": "40.0.0"
  },
  "devDependencies": {
    "@types/node": "16.11.24",
    "confusing-browser-globals": "1.0.11",
    "read-pkg-up": "7.0.1",
    "dot-prop": "6.0.1",
    "semver": "7.3.5",
    "@rushstack/eslint-patch": "1.1.0",
    "prettier": "2.5.1",
    "typescript": "4.5.5"
  },
  "optionalDependencies": {
    "ansi-regex": ">=6.0.1",
    "trim": ">=0.0.3",
    "underscore": ">=1.12.1"
  },
  "peerDependencies": {
    "@types/lodash": "^4.14.176",
    "@types/react": "^17.0.34",
    "lodash": "^4.17.21",
    "prettier": "2.5.1",
    "react": "^17.0.2"
  }
}
